{
  // This is a service account credential file for Google Cloud services
  "type": "service_account",
  "project_id": "YOUR_PROJECT_ID", // Replace with your Google Cloud project ID
  "private_key_id": "YOUR_PRIVATE_KEY_ID", // Replace with your private key ID
  "private_key": "YOUR_PRIVATE_KEY", // Replace with your private key (including BEGIN/END PRIVATE KEY tags and newlines)
  "client_email": "YOUR_SERVICE_ACCOUNT_EMAIL", // Replace with your service account email
  "client_id": "YOUR_CLIENT_ID", // Replace with your client ID
  "auth_uri": "https://accounts.google.com/o/oauth2/auth", // Standard Google OAuth 2.0 auth URI
  "token_uri": "https://oauth2.googleapis.com/token", // Standard Google OAuth 2.0 token URI
  "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs", // Standard x509 certificate URL
  "client_x509_cert_url": "YOUR_CLIENT_CERT_URL", // Replace with your client x509 cert URL
  "universe_domain": "googleapis.com" // Standard universe domain
}
